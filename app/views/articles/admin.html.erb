<h1>approve these ... or not
<%=  current_user.email if current_user %>
</h1>
<% if current_user %>

<% @articles.each do |article| %>
<div id="<%= article.id %>">
  <%= render 'shared/post', :article => article %>
  <div>
    <button class="btn btn-success" onClick="approve(<%=article.id%>);">Approve</button>
    <button class="btn btn-danger" onClick="destroy(<%=article.id%>);">Delete</button>
  </div>
</div>
<br><br>
<% end %>
<% else %>
not logged in
<% end %>
