<h1>Log In</h1>
    <div class="form-group">
    
    <%=form_tag log_in_path do%>
    
    <%=label_tag :email %>
    
    <%=text_field_tag :email %>
    
    <%=label_tag :password %>
    
    <%=password_field_tag :password %>
    
    <%=submit_tag "Log in",  class: "btn btn-standard" %>
    
    <%end%>
</div>