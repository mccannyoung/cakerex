<div class="panel panel-primary">
    <div class="panel-heading"><b><%= article.title %></b></div>
    <div class="panel-body">
        <%= link_to image_tag(article.image.url, class: 'media-object'), article.image.url, target: '_blank' %>
        <br />
        <%= article.text %>
        <br /><h6><b>from:</b> <%= article.url %></h6>
        submitted by:  
        <% if article.userid >0 %>
            <%= article.userid %>
        <% else %>
            Annonymous
        <%end%>
    </div>
</div>